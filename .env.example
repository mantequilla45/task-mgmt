# Supabase Public Keys (safe to expose)
NEXT_PUBLIC_SUPABASE_ANON_KEY="your_supabase_anon_key_here"
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY="your_supabase_publishable_key_here"
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"

# Database Configuration
POSTGRES_DATABASE="postgres"
POSTGRES_HOST="db.your-project.supabase.co"
POSTGRES_PASSWORD="your_database_password_here"
POSTGRES_USER="postgres"

# Database Connection URLs
# Pooled connection for Prisma (recommended for serverless)
POSTGRES_PRISMA_URL="postgres://postgres.your-project:password@aws-region.pooler.supabase.com:6543/postgres?sslmode=require&pgbouncer=true"

# Standard pooled connection
POSTGRES_URL="postgres://postgres.your-project:password@aws-region.pooler.supabase.com:6543/postgres?sslmode=require"

# Direct connection (for migrations)
POSTGRES_URL_NON_POOLING="postgres://postgres.your-project:password@aws-region.pooler.supabase.com:5432/postgres?sslmode=require"

# Supabase Service Keys (keep secret!)
SUPABASE_JWT_SECRET="your_jwt_secret_here"
SUPABASE_PUBLISHABLE_KEY="your_publishable_key_here"
SUPABASE_SECRET_KEY="your_secret_key_here"
SUPABASE_SERVICE_ROLE_KEY="your_service_role_key_here"
SUPABASE_URL="https://your-project.supabase.co"